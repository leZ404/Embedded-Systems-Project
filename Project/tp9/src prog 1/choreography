DBT;         //debut programme

DAL 1;      // allumer la  del (faut verifier le 1)
MAV 150;    // avancer le robot avec un pwm de 60,78 %
SGO 54;     // jouer sonorite a 180 Hz
ATT 100;    // attendre pendant 2.5 sec 
SAR;        // arrêter de jouer la sonorite
MRE 150 ;   // reculer le robot avec un pwm de 60,78 %
ATT 100;    // attendre pendant 2.5 sec 
DET 1;      // eteindre la del 
MAR;        //arrêter roues moteur 
DBC 5;        //boucle
TRG;        // tourner a gauche
ATT 200;    // attendre pendant 5 sec
MAR;        //arrêter roues moteur
TRD;       // tourner a droite 
ATT 200;    // attendre pendant 5 sec
FBC;        // fin boucle 
SGO 27;      // jouer sonorite a 90 Hz
DAL 1;       // allumer la  del
ATT 100;     // attendre pendant  2.5 sec
DET 1;       //   eteindre la del
FIN;         // fin programme
